{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "-webkit-overflow-scrolling",
          "-ms-filter",
          "height: fit-content",
          "-webkit-tap-highlight-color",
          "scale"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/structure": "off",
    "disown-opener": "off"
  }
}